{
  "expo": {
    "name": "sheet-snap",
    "owner": "plandoer",
    "slug": "sheet-snap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "sheetsnap",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.plandoer.sheetsnap"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "package": "com.plandoer.sheetsnap",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": ["@react-native-google-signin/google-signin", "expo-router"],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4c0af868-fd54-423a-b093-d1acf45dd047"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/4c0af868-fd54-423a-b093-d1acf45dd047"
    }
  }
}
